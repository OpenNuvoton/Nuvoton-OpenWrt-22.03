				label = "uboot-env";
				reg = <0x300000 0xc0000>;
			};
			device-tree@3c0000 {
				label = "device-tree";
				reg = <0x3c0000 0x40000>;
			};
			kernel@400000 {
				label = "firmware";
				reg = <0x400000 0x0>;
			};
		};

		spidev0: spi@1 {
			compatible = "semtech,sx1301";
			reg = <1>;
			spi-max-frequency = <12000000>;
		};
	};

